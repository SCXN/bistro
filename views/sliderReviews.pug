div.item.sliderCase
  h2.fixed What our guests say about thier visit
  div(data-ride='carousel')#carouselExampleIndicators.carousel.slide
    ol.carousel-indicators
      li(data-target='#carouselExampleIndicators' data-slide-to='0').active 
      li(data-target='#carouselExampleIndicators' data-slide-to='1')
      li(data-target='#carouselExampleIndicators' data-slide-to='2')
    div.carousel-inner
      div.carousel-item.active   
        img(src='https://images.unsplash.com/photo-1504973960431-1c467e159aa4?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1170&q=80' alt='First slide').d-block.w-100.sliderImg
        div.carousel-caption.superCenterer
          div.reviewCase
            small Rated—
            h1.rating #{rev[0].rating}/5
            p.d-none.d-md-block "#{rev[0].text}" 
            h3 —
              a(href=rev[0].user.profile_url) #{rev[0].user.name} 
              strong on 
              a(href=rev[0].url) yelp
            img(src='https://matrix-client.matrix.org/_matrix/media/r0/download/matrix.org/vtuyHjVabFmwTrXBzJiFtGLd').reviewCaseFlankLeft
            img(src='https://matrix-client.matrix.org/_matrix/media/r0/download/matrix.org/vtuyHjVabFmwTrXBzJiFtGLd').reviewCaseFlankRight
            img(src='/images/knifeAndSpoon.png').placeSettingRight
            img(src='/images/fork.png').placeSettingLeft
      div.carousel-item
        img(src='https://images.unsplash.com/photo-1414235077428-338989a2e8c0?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1170&q=80' alt='Second slide').d-block.w-100.sliderImg
        div.carousel-caption.superCenterer
          div.reviewCase
            small Rated—
